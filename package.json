{
  "name": "@oslo-flanders/org-to-ttl-converter",
  "author": "Digitaal Vlaanderen <https://data.vlaanderen.be/id/organisatie/OVO002949>",
  "version": "0.0.4",
  "description": "Convert organization JSON data to RDF Turtle format",
  "main": "dist/index.js",
  "bin": {
    "oslo-company-to-json": "dist/convert-company-to-json.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "convert:dummy": "ts-node convert-company-to-json.ts --input ./dummy.json --output ./output/organisations",
    "convert:all": "ts-node convert-company-to-json.ts --input ./organisationd_zoekresultagen.json --output ./output/organisations",
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "start": "node dist/convert-company-to-json.js",
    "postinstall": "chmod +x dist/convert-company-to-json.js || true"
  },
  "keywords": [
    "rdf",
    "turtle",
    "ttl",
    "OVO",
    "oslo",
    "digitaal-vlaanderen"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Informatievlaanderen/OSLO-codelijsten.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/node": "^20.19.25",
    "@types/streamify-array": "^1.0.3",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@oslo-flanders/core": "^1.0.15",
    "csv-parser": "^3.2.0",
    "inversify": "^6.0.1",
    "n3": "^1.16.2",
    "rdf-data-factory": "^1.1.1",
    "rdf-parse": "^2.3.2",
    "rdf-serialize": "^2.2.2",
    "streamify-array": "^1.0.1",
    "streamify-string": "^1.0.1",
    "yargs": "^18.0.0"
  }
}
