{
  "name": "org-to-ttl-converter",
  "version": "1.0.0",
  "description": "Convert organization JSON data to RDF Turtle format",
  "main": "convert-org-to-ttl.ts",
  "scripts": {
    "convert": "ts-node convert-org-to-ttl.ts",
    "convert:dummy": "ts-node convert-org-to-ttl.ts ./dummy.json ./output/organisations.ttl",
    "build": "tsc",
    "start": "node dist/convert-org-to-ttl.js"
  },
  "keywords": [
    "rdf",
    "turtle",
    "ttl",
    "organisation",
    "oslo"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.19.25",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}
